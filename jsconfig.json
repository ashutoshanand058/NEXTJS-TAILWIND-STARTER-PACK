{
    "compilerOptions": {
      "baseUrl": ".",
      "target": "es6",
      "module": "es6",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "noImplicitAny": false,
      "paths": {
        "@components/*": ["./src/components/*"],
        "@assets/*": ["./public/assets/*"]
    }
  },
  "typeAcquisition": {
    "enable": true
  },
  "typingOptions": {
    "enableAutoDiscovery": true
  },
   "allowJS": true,
   "checkJS": true,
    "exclude": ["node_modules"]
  }